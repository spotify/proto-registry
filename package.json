  "dependencies": {
    "@blueprintjs/core": "^3.6.1",
    "@blueprintjs/select": "^3.2.0",
    "fuse.js": "^3.2.1",
    "history": "^4.7.2",
    "protobufjs": "^6.8.8",
    "react": "^16.5.2",
    "react-commonmark": "^3.0.0",
    "react-dom": "^16.5.2",
    "react-markdown": "^3.6.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^4.0.0",
    "redux-localstorage": "^1.0.0-rc5",
