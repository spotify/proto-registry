    "@types/react-router-dom": "^4.3.1",
    "@types/react-router-redux": "^5.0.16",
    "@types/redux-localstorage": "^1.0.8",
    "@types/redux-localstorage-filter": "^0.1.4",
    "eslint-plugin-typescript": "^0.12.0",
    "husky": "^1.0.1",
    "jest-runner-standard": "^0.0.12",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
